{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Acompanhando as aulas do https://curso.dev com o filipe deschamps. Implementação do https://www.tabnews.com.br do zero",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "db-docker:up": "docker compose -f infra/compose.yaml up -d",
    "db-docker:recreate": "docker compose -f infra/compose.yaml up -d --force-recreate",
    "db-docker:down": "docker compose -f infra/compose.yaml down"
  },
  "author": "Beatriz Biagini",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.7.4"
  }
}
